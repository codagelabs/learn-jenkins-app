{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJemkinsApp-TaskDefination-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "595199363770.dkr.ecr.us-east-1.amazonaws.com/myjenkinsapp:1.2.91",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::595199363770:role/ecsTaskExecutionRole"
}deploy 